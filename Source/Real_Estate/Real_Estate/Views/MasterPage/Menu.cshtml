@model List<Real_Estate.Models.PropertyModel>
@{
    ViewBag.Title = "Menu";
}
@{
    Layout = null;
}
<div id="menu">
    <div class="moduletable">
        <ul id="nav" class="dropdown dropdown-horizontal">
            <li class="menu_active" id="menu_1">
                <span><a href="">Homepage</a></span>
            </li>
            @foreach (var item in Model)
            {
                <li class="dir" id="@item.id">
                    <span>@item.name_eng</span>
                    <ul class="menu_muti_col">
                        @if (item.ls_Location.Count >0)
                        {
                            <li style="width:800px;height:170px;">
                                @foreach (var sub in item.ls_Location)
                                {

                                    {
                                        <div style="float:left;width:140px;background-image:none" class="megacol">
                                            <a style="" href="">@sub.location_name_eng</a>
                                        </div>
                                    }

                                }

                            </li>
                        }
                        else
                        {
                            
                        }
                    </ul>
                </li>

            }
        </ul>
    </div>
</div>


