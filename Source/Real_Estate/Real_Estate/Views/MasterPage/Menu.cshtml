<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery('.item_more').click(function () {
            var id = jQuery(this).attr('rel');
            var item = ".item_hidden" + id;
            jQuery(item).show();
            jQuery(this).hide();
        });
        jQuery('.item_close').click(function () {
            var id = jQuery(this).attr('rel');
            var item = ".item_hidden" + id;
            jQuery(item).hide();
            jQuery(item).trigger('click');
        });
    });
</script>